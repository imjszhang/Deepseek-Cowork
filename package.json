{
  "name": "deepseek-cowork",
  "version": "0.1.35",
  "description": "Open-Source Alternative to Claude Cowork - Implement all Claude Cowork features using DeepSeek",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --enable-logging",
    "server": "node server/index.js",
    "server:pages": "node scripts/server-pages.js",
    "cli": "node packages/cli/bin/cli.mjs",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "prebuild": "node scripts/generate-icon.js && node scripts/update-version.js",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "build:all": "electron-builder --win --mac --linux",
    "build:web": "node build/web.config.js"
  },
  "keywords": [
    "deepseek",
    "deepseek-cowork",
    "claude-cowork",
    "claude-cowork-alternative",
    "open-source",
    "ai-assistant",
    "browser-automation",
    "file-management",
    "llm",
    "claude-code",
    "electron",
    "browser-control",
    "automation",
    "browser-extension",
    "ai",
    "self-hosted",
    "privacy-first",
    "local-ai",
    "web-scraping",
    "form-automation",
    "deepseek-api"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "./",
  "dependencies": {
    "axios": "^1.6.0",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "electron-updater": "^6.7.3",
    "express": "^4.18.2",
    "happy-coder": "file:./lib/happy-cli",
    "libsodium-wrappers": "^0.7.13",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "sql.js": "^1.11.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.33.5"
  },
  "overrides": {
    "rimraf": "^5.0.0",
    "glob": "^13.0.0",
    "tar": "^7.5.2",
    "boolean": "^4.0.0",
    "shelljs": "^0.10.0",
    "shx": {
      "shelljs": "^0.10.0",
      "glob": "^13.0.0"
    },
    "happy-coder": {
      "shx": {
        "shelljs": "^0.10.0",
        "glob": "^13.0.0"
      },
      "glob": "^13.0.0"
    }
  }
}
